package com.cls;

class StackArray{
	
	private int maxSize;
	private int[] arr;
	private int top;
	
	public StackArray(int size) {
		this.maxSize = size;
		this.arr = new int[maxSize];
		this.top = -1;
	}
	
	public boolean isFull()
	{
		return (top == maxSize-1);
	}
	
}
public class Exp9 {

	public static void main(String[] args) {
		
	
	}

}
