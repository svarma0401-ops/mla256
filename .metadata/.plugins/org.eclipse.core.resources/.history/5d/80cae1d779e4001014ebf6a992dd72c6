package com.searching;

public class LinearSearch {

	public static int searchElement(int[] arr, int target) {
		for(int i=0;i<arr.length;i++) {
			if(arr[i] == target) {
				return i ;
			}
			
		}
		return -1;
	}
	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}

}
