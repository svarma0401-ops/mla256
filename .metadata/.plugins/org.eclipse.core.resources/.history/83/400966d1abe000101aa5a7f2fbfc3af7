package com.dtm;

import java.util.Arrays;
import java.util.List;

public class Exp3 {
	
	private static double add(List<? extends Number> list) {
		double a = 0 ; 
		
		for(Number n : list) {
			
			a = n.doubleValue();
			a += a;
		}
		return a;
	}
	
	public static void main(String[] args) {
		List<Integer> list1 = Arrays.asList(12,3,4,5);
		add(list1);
	}
}
