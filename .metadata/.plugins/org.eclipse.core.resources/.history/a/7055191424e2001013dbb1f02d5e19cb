package com.cls;

import java.util.HashSet;

public class Exp3 {

	public static void main(String[] args) {
	
		HashSet data = new HashSet();
		
		data.add(100);
		data.add("JAVA");
		data.add('a');
		data.add(1000);
		data.add(100);
		data.add(22.222f);
		data.add(true);
		data.add("hello");
		System.out.println(data);
		System.out.println(data.size());

	}

}
