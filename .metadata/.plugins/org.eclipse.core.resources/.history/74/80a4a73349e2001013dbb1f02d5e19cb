package com.cls;

import java.util.ArrayList;
import java.util.Collections;
import java.util.Comparator;
import java.util.List;

class Book1 implements Comparable<Book> {
	private int id;
	private String name;
	private int price;
	
	public Book1(int id, String name, int price) {
		this.id =id;
		this.name=name;
		this.price = price;
		
	}

	public int getId() {
		return id;
	}

	public void setId(int id) {
		this.id = id;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public int getPrice() {
		return price;
	}

	public void setPrice(int price) {
		this.price = price;
	}
	
	@Override
	public String toString() {
		return "Book [id=" +id+",name=" +name + ", price=" + price +"]";
	}
	
	@Override
	public int compareTo(Book1 o) {
		return this.id = o.id;
	}
}

