package com.sortings;

public class BubbleSort {

	
	public static void sortElements(int[] arr) {
		int n = arr.length;
		int t = 0;
		for(int i = 0; i<n;i++) {
			for(int j = 0 ; j <n-i;j++) {
				if(arr[j] >arr[j+1]) {
					t = arr[j];
					arr[j] = arr[j+1];
					arr[j+1]= t;
				}
			}
		}
	}
	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}

}
