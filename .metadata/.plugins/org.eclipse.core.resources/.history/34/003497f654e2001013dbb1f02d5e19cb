package com.cls;

import java.util.ArrayList;
import java.util.Arrays;
import java.util.Collections;
import java.util.Comparator;
import java.util.List;
import java.util.function.Function;

class Book1 implements Comparable<Book1> {
	private int id;
	private String name;
	private int price;
	
	public Book1(int id, String name, int price) {
		this.id =id;
		this.name=name;
		this.price = price;
		
	}

	public int getId() {
		return id;
	}

	public void setId(int id) {
		this.id = id;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public int getPrice() {
		return price;
	}

	public void setPrice(int price) {
		this.price = price;
	}
	
	@Override
	public String toString() {
		return "Book [id=" +id+",name=" +name + ", price=" + price +"]";
	}
	

}

public class Exp8 {
     public static void main(String[] args) {
	
    	 List<Book1> data = Arrays.asList(new Book1(101,"java", 340),new Book1(201,"php", 10),new Book1(501,"python", 43),new Book1(401,"kotlin", 34),new Book1(301,"core java", 55));
    	 
    	 //data.stream().sorted(Comparator.comparing(Book1::getId)).forEach(System.out::println);
    	 
    	 Function < Book1, String> fnf = Book1::getName;
    	// data.stream().sorted(Comparator.comparing(fnf)).forEach(System.out::println);
    	 
    	//System.out.println();
    	 
    	 Function < Book1, Integer> fni = Book1::getId;
    	// data.stream().sorted(Comparator.comparing(fni)).forEach(System.out::println);
    	// System.out.println();
    	 
    	 Function < Book1, Integer> fnp = Book1::getPrice;
    	 //data.stream().sorted(Comparator.comparing(fnp)).forEach(System.out::println);
    	 
    	 data.stream().sorted(Comparator.comparing(fni).thenComparing(fnp).thenComparing(fnf)).forEach(System.out::println);
}
}
