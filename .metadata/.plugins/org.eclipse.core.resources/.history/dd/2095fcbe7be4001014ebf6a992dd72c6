package com.searching;

public class BinarySearch {

	public static int searchElement(int[]arr,int target) {
		int l = 0 ;
		int h = arr.length -1;
		while(l<=h) {
			int m = l +(h-1)/2;
			if(arr[m] == target) {
				return m;
			}
			if(arr[m]<target) {
				l=m+1;
			}
			else
			{
				h = m-1;
			}
		}
	}
	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}

}
