package com.ths;

public class Lifecycle implements Runnable {
	@Override
	public void run() {
		try
		{
			System.out.println("run()");
		}
		catch(Exception e)
		{
			e.printStackTrace();
		}
	}
	public static void main(String[] args) {
		
		Lifecycle obj = new Lifecycle();
	//	obj.run();
		
		Thread t1 = new Thread(obj);

	}

}
