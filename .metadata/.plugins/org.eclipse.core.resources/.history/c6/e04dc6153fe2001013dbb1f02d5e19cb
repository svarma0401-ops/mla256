package com.cls;

import java.util.TreeSet;

class Employee 
{
	private int id;
	private String name;
	private int salary;
	
	public Employee(int id, String name,int salary)
	{
		this.id = id;
		this.name = name;
		this.salary=salary;
	}

	public int getId() {
		return id;
	}

	public void setId(int id) {
		this.id = id;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public int getSalary() {
		return salary;
	}

	public void setSalary(int salary) {
		this.salary = salary;
	}
	
	@Override
	public String toString() {
		return "Employee [id=" +id + ", name=" +name+ ",salary= " +salary+ "]";
	}
}
public class Exp7 {

	public static void main(String[] args) {
		
	TreeSet<Employee> data = new TreeSet<>();
	
	data.add(new Employee(501,"rohit",235));
	data.add(new Employee(502,"dhoni",345));
	data.add(new Employee(509,"pant",333));
	data.add(new Employee(504,"rahul",400));
	data.add(new Employee(505,"virat",455));
	
	data.forEach(System.out::println);
	
	}

}
